
page {
	width: 100%;
	height: 100%;
	background: #eee;
	color: #333;
}
.fs-text {
	font-size: 36rpx;
	color: #999;
	text-align: center;
	line-height: 100rpx;
}
.main {
	padding: 15rpx 2% 120rpx;
}
.main::-webkit-scrollbar {
	display: none;
}
.liuyan textarea {
	background: #fff;
	width: 100%;
	padding: 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	color: #444;
	font-size: 32rpx;
	line-height: 45rpx;
	border-radius: 5rpx;
}
.liuyan button {
	width: 300rpx;
	height: 80rpx;
	line-height: 80rpx;
	padding: 0;
	margin: 30rpx auto 0;
	outline: none;
	border: 0;
	background: #009999;
	color: #fff;
	font-size: 36rpx;
}
.list {}
.list-on {
	background: #fff;
	margin-bottom: 15rpx;
	border-radius: 5rpx;
}
.list-on view {
	padding: 0 20rpx;
	border-bottom: 1rpx solid #E6E6E6;
}
.list-on view:nth-last-of-type(1) {
	border-bottom: 0;
}
.list-date {
	font-size: 30rpx;
	color: #454545;
	line-height: 80rpx;
}
.list-bar {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.list-bar text {
	width: 90%;
	height: 100%;
	font-size: 34rpx;
	line-height: 150rpx;
	overflow: hidden;
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
}
.footer {
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 120rpx;
	background: #fff;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.footer view {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	line-height: 120rpx;
	text-align: center;
}
.list-active {
	background: #009999;
	color: #fff;
}

