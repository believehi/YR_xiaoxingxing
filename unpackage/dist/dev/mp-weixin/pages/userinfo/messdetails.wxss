
page {
	width: 100%;
	height: 100%;
	background: #eee;
	padding: 15px 2%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.title-info {
	font-size: 36rpx;
	color: #999;
	text-align: center;
	line-height: 200rpx;
}
.content {
	width: 100%;
	background: #fff;
	min-height: 300rpx;
	color: #333;
	border-radius: 10rpx;
	margin-bottom: 20rpx;
}
.title {
	font-size: 36rpx;
	font-weight: 600;
	line-height: 90rpx;
	padding: 0 20rpx;
	border-bottom: 1rpx solid #E6E6E6;
}
.de-date {
	padding: 0 20rpx;
	font-size: 30rpx;
	line-height: 80rpx;
	color: #646464;
}
.details {
	min-height: 100rpx;
	font-size: 34rpx;
	line-height: 60rpx;
	padding: 0rpx 20rpx 15rpx;
}

