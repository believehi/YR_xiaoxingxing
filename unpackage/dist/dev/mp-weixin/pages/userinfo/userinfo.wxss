
page {
	width: 100%;
	height: 100%;
	background: #eeeeee;
}
.cart{
	text-align: center;
	font-size: 30rpx;
	padding: 250rpx 0;
}
#jm {
	width: 90%;
	text-align: left;
	padding-left: 4%;
}
.uni-bc-but {
	background-color: #FBB42B;
	border-radius: 10rpx;
	font-size: 30rpx;
	letter-spacing: 3rpx;
	color: #FFFFFF;
	border: none;
	width: 250rpx;
	height: 50rpx;
	line-height: 50rpx;
	margin: 50rpx auto;
}
.content {
	padding: 0 2% 15rpx;
}
.uni-info-title {
	position: relative;
}
.uni-back {
	margin-top: 1.2%;
	background: #FFFFFF;
	color: #bfbfbf;
}
.uni-info-box {
	padding: 1.5% 1%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 26rpx;
}
.uni-user-text {
	margin-left: 200rpx;
	color: #666;
	font-size: 26rpx;
	line-height: 36rpx;
	padding: 15rpx 0 10rpx;
}
.uni-user-dz {
	display: inline-block;
	width: 100%;
}
.uni-bj-but {
	display: inline-block!important;
	width: 210rpx;
	height: 56rpx;
	background-color: #009999;
	border-radius: 8rpx;
	text-align: center;
	line-height: 56rpx;
	color: #FFFFFF;
	margin-top: 3%;
	font-size: 28rpx;
	-webkit-box-shadow: 0rem 5rpx #006666;
	        box-shadow: 0rem 5rpx #006666;
}
.uni-bm {
	margin-right: 50rpx;
}
.uni-user-img {
	position: absolute;
	top: 20rpx;
	left: 10rpx;
	width: 170rpx;
	height: 170rpx;
	background-color: #dddddd;
	border-radius: 50%;
	overflow: hidden;
}
.uni-user-img image {
	width: 100%;
	height: 100%;
}
.uni-jf-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	font-size: 28rpx;
	padding: 1.5% 0 2% 0;
}
.uni-bj-but1 {
	margin-top: 0% !important;
}
.uni-jf-box text {
	text-align: center;
	width: 200rpx;
	font-size: 26rpx;
	margin-top: 1%;
}
.uni-bj-but1:nth-child(3) {
	margin-left: 7%;
}
.uni-dw-box {
	font-size: 28rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding: 1% 0;
}
.uni-dw-box text {
	width: 21%;
	text-align: center;
}
.uni-lv-icon {
	width: 40rpx;
	text-align: center;
	position: relative;
	height: 68rpx;
	margin-right: 29rpx;
}
.uni-lv-icon text {
	font-size: 26rpx;
	position: absolute;
	bottom: 0;
	left: 0;
}
.uni-ry-bt {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding: 1.7% 0;
}
.uni-ry-bt view {
	width: 25%;
	text-align: center;
	font-size: 28rpx;
}
.uni-ry-x {
	background: url("http://xxxpv.01.t1m.cn/Img/page_x.png") no-repeat bottom;
	background-size: 100%;
}
.ly-icon {
	position: fixed;
	width: 120rpx;
	height: 120rpx;
	bottom: 80rpx;
	right: 10rpx;
	border-radius: 50%;
	overflow: hidden;
}
.ly-icon image {
	width: 100%;
	height: 100%;
	opacity: 0.8;
}

