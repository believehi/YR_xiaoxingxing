
page {
	width: 100%;
	height: 100%;
	background: #eeeeee;
}
image {
	display: block;
}
.uni-sp-list {
	margin-bottom: 130rpx;
}
.cart {
	text-align: center;
	margin-top: 10%;
	font-size: 30rpx;
}
.uni-sp-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding: 5%;
	height: 50rpx;
	border-bottom: solid 1px #e6e6e6;
	vertical-align: middle;
	background: #FFFFFF;
}
label {
	margin-top: -3rpx;
}
checkbox {
	width: 45rpx;
	height: 45rpx;
	border-radius: 50%;
}
.fa {
	color: #009999;
	font-size: 40rpx;
	margin-top: 7rpx;
}
.fa-minus-square-o {
	margin-left: 20rpx;
}
.uni-number {
	font-size: 26rpx;
	width: 7%;
	text-align: center;
}
.uni-qu-but {
	background: #3ee459 !important;
}
.uni-title {
	font-size: 32rpx;
	color: #000000;
	margin-left: 5.5%;
	width: 45%;
	overflow: hidden;
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
}
.uni-page {
	font-size: 30rpx;
	width: 25%;
	text-align: right;
}
.uni-hj-box {
	position: fixed;
	bottom: 0;
	background: #FFFFFF;
	height: 105rpx;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	font-size: 32rpx;
	line-height: 105rpx;
	border-top: 1rpx solid #eeeeee;
}
.uni-jx-but {
	width: 25%;
	height: 100%;
	text-align: center;
	color: #FFFFFF;
	letter-spacing: 15rpx;
	text-indent: 15rpx;
	background-color: #009999;
}
.uni-sc-but {
	width: 25%;
	height: 100%;
	text-align: center;
	color: #FFFFFF;
	letter-spacing: 15rpx;
	text-indent: 15rpx;
	background-color: #f02d4d;
}
.uni-hj {
	width: 50%;
	margin-left: 30rpx;
}

/*checkbox 选项框大小  */
.checkbox {
	width: 35rpx;
	height: 35rpx;
	border-radius: 50%;
	background-color: #FFFFFF;
	border: solid 2rpx #cecece;
	overflow: hidden;
}
.checkbox image {
	width: 35rpx;
	height: 35rpx;
	border-radius: 50%;
}
#None {
	display: none;
}
#Show {
	display: block;
}

