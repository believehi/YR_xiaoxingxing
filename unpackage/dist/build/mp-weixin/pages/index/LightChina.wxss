
page,
	{
	width: 100%;
	height: 100%;
	position: relative;
}
.uni-ss-box {
	position: absolute;
	top: 5%;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
}
.uni-ss-box input {
	background: #FFFFFF;
	width: 534rpx;
	height: 75rpx;
	font-size: 30rpx;
	padding-left: 80rpx;
}
.uni-inout-box {
	position: relative;
	width: 614rpx;
	color: #666666;
}
.uni-ss-box uni-icon {
	position: absolute;
	top: 20rpx;
	left: 20rpx;
}
.qd_but {
	margin-top: 50rpx;
	width: 314rpx;
	height: 68rpx;
	color: #FFFFFF;
	background-color: #006666;
	border-radius: 5px;
	letter-spacing: 35rpx;
	text-indent: 35rpx;
	-webkit-box-shadow: 0rem 5rpx #006666;
	        box-shadow: 0rem 5rpx #006666;
	font-size: 30rpx;
}

