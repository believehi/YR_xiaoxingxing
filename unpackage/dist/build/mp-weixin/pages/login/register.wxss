
page {
	color: #000000;
	font-family: "SourceHanSansCN-Medium";
}
.content {
	padding: 0 5%;
}
.uni-input {
	margin-top: 25rpx;
	position: relative;
}
.ss_xq {
	position: absolute;
	z-index: 999999;
	top: 139rpx;
	left: 0;
	width: 100%;
	max-height: 300rpx;
	background-color: #eee;
	border-radius: 8rpx;
	overflow: auto;
	color: #333;
	font-size: 34rpx;
}
.ss_xq view {
	width: 100%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 0 20rpx;
	overflow: hidden;    
	-o-text-overflow: ellipsis;    
	   text-overflow: ellipsis;
	white-space: nowrap;
	line-height: 60rpx;
}
.ss_xq view:nth-of-type(1) {
	margin-top: 10rpx;
}
.ss_xq view:nth-last-of-type(1) {
	margin-bottom: 10rpx;
}
.phone {
	font-size: 75rpx;
	top: 65rpx !important;
	left: 25rpx !important;
}
.uni-input input {
	border-radius: 10rpx;
	border: solid 1rpx #dcdcdc;
	height: 75rpx;
	margin-top: 10rpx;
	font-size: 30rpx;
	padding-left: 15rpx;
}
.uni-lx1 input {
	padding-left: 85rpx !important;
}
.uni-input uni-icon {
	position: absolute;
	left: 20rpx;
	top: 76rpx;
	color: #a0a0a0;
}
.uni-input text {
	font-size: 34rpx;
}
.uni-code {
	position: absolute;
	z-index: 9999;
	right: 15rpx;
	top: 74rpx;
	font-size: 26rpx;
	width: 207rpx;
	height: 54rpx;
	background-color: #009999;
	border-radius: 5px;
	text-align: center;
	line-height: 54rpx;
	color: #FFFFFF;
}
checkbox .uni-checkbox-input {
	border-radius: 50%;
}
.uni-user-xy {
	margin-top: 35rpx;
	font-size: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.uni-user-xy text {
	margin-left: 15rpx;
}
.qd_but {
	margin-top: 100rpx;
	width: 530rpx;
	height: 85rpx;
	line-height: 85rpx;
	color: #FFFFFF;
	background-color: #006666;
	border-radius: 10rpx;
	letter-spacing: 35rpx;
	text-indent: 35rpx;
	-webkit-box-shadow: 0rem 5rpx #006666;
	        box-shadow: 0rem 5rpx #006666;
}

