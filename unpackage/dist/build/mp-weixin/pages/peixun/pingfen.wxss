
page {
	width: 90%;
	height: 100%;
	padding: 0 5%;
}
.main-title {
	font-size: 36rpx;
	color: #999;
	text-align: center;
	line-height: 100rpx;
}
.uni-title {
	font-size: 34rpx;
	color: #000000;
	font-weight: bold;
	padding: 3% 5%;
	text-align: center;
}
.uni-fu-title {
	font-size: 28rpx;
	color: #676767;
}
.uni-tm-content {
	margin-top: 6%;
}
.uni-tm-title {
	line-height: 27rpx;
}
.uni-tm-title,
.uni-title1 {
	font-size: 30rpx;
	color: #000000;
	line-height: 45rpx;
}
.uni-tm-itme text {
	font-size: 24rpx;
	margin-left: 4%;
	color: #676767;
	line-height: 55rpx;
	display: block;
}
.uni-iput-box .uni-title1 {
	margin-left: 4%;
}
.uni-iput-box {
	margin-bottom: 20rpx;
}
.uni-iput-box input {
	border: 1rpx solid #e6e6e6;
	height: 60rpx;
	margin-top: 5rpx;
	font-size: 28rpx;
	padding-left: 15rpx;
}
.uni-bc-but {
	background-color: #009899;
	border-radius: 20rpx;
	font-size: 34rpx;
	letter-spacing: 3rpx;
	color: #FFFFFF;
	border: none;
	height: 90rpx;
	margin: 40rpx 0 50rpx;
}

