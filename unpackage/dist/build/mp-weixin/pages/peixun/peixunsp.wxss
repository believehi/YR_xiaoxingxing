
page {
	width: 100%;
	height: 100%;
	background: #eeeeee;
}
.inif-on {
	font-size: 36rpx;
	color: #666;
	text-align: center;
	line-height: 100rpx;
}
.content {
	display: table;
	width: 100%;
	margin-top: 15rpx;
}
.main {
	display: inline-block;
	width: 50%;
	height: 240rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 0 15rpx;
}
.main-init {
	height: 1rpx;
}
.uni-item {
	position: relative;
	background: #FFFFFF;
	width: 100%;
	height: 100%;
}
.uni-item video {
	width: 100%;
	height: 100%;
	/* display: none; */
}
.aaaa {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}
.aaaa image {
	width: 100%;
	height: 100%;
}


